<section id="content">

<% blog.articles[0..5].each do |article| %>
  <article class="post">
    <header>
      <h1><a href="<%= article.url %>"><%= article.title %></a></h1>
      <time datetime="<%= article.date %>"><%= article.date.strftime('%B %e, %Y') %></time>
    </header>

    <%= article.summary %>
  </article>
<% end %>

</section>
